<h1>My Jobs</h1>

  <table>
    <thead>
      <tr>
        <th>Applied</th>
        <th></th>

        <th>Position</th>
        <th></th>

        <th>Company</th>
        <th></th>


        <th></th>

        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @user_jobs.order("created_at DESC").each do |user_job| %>
        <tr>
          <td><%= time_ago_in_words(user_job.created_at) %></td>
          <td></td>

          <td><%= user_job.position %></td>
          <td></td>
          <td><%= user_job.company %></td>
          <td></td>


          <td></td>
          <td><%= link_to ' Show ', user_job %>
              <br>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
  <hr>

  <%= link_to 'Add New Job', new_job_path %> |  <%= link_to ' Back', :back %>
